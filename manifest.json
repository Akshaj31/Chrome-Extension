{
	"manifest_version": 3,
	"name": "Image Converter",
	"version": "1.0",
	"description": "Convert JPEG to PNG and vice versa.",
	"permissions": ["downloads"],
	"action": {
		"default_popup": "popup.html",
		"default_icon": {
			"24": "icon_24.png",
			"48": "icon_48.png"
		}
	},
	"icons": {
		"24": "icon_24.png",
		"48": "icon_48.png"
	}
}
